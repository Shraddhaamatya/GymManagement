@model IEnumerable<GymManagement.Models.ViewModel.WorkoutViewModel>

@{
    ViewBag.Title = "ViewWorkout";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

<div class="module">
    <div class="module-head">
        <h3>View Workout</h3>
    </div>
    <div class="module-body">

        <div class="col-md-9">

    
<div class="module-body table">
    <table id="myTable" cellpadding="0" cellspacing="0" border="0" class="datatable-1 table table-bordered table-striped	 display" width="100%">
    <tr>
        
        
        <th>
            @Html.DisplayNameFor(model => model.WorkoutDays)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        
    </tr>

@foreach (var item in Model) {
    <tr>
      
        <td>
            @Html.DisplayFor(modelItem => item.WorkoutDays)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        
    </tr>
}

</table>
    </div>
            </div>
        </div>
    </div>

