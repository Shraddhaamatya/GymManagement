<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/bootstrap/css/bootstrap.min.css")" rel="stylesheet" />
<link href="@Url.Content("~/bootstrap/bootstrap-theme.min.css")" rel="stylesheet" />

<link href="@Url.Content("~/bootstrap/css/bootstrap-responsive.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/css/theme.css")" rel="stylesheet" />
     <link href="@Url.Content("~/images/icons/css/font-awesome.css")" rel="stylesheet" />

<script src="@Url.Content("~/Scripts/jquery-1.9.1.min.js")"></script>
    <script src="~/scripts/jquery.validate.min.js"></script>
    <script src="~/scripts/jquery.validate.unobtrusive.min.js"></script>

    
       
       
       
        
</head>
<body>
    <div class="navbar navbar-fixed-top">
            <div class="navbar-inner">
                <div class="container">
                    <a class="btn btn-navbar" data-toggle="collapse" data-target=".navbar-inverse-collapse">
                        <i class="icon-reorder shaded"></i></a><a class="brand" href="@Url.Action("Index","User")">Gym Management </a>
                    <div class="nav-collapse collapse navbar-inverse-collapse">
                       
                       
                        <ul class="nav pull-right">
                           
                            <li><a href="#">
                                @if(Session["fullname"]!=null)
                                {
                                @Session["fullname"].ToString()
                                }
                                </a></li>
                            <li class="nav-user dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                 @if(Session["photo"]!=null)
                                {
                                     <img src="~/MemberPhoto/@Session["photo"].ToString()" class="nav-avatar" />
                               
                                }
                                else
                                {
                                <img src="~/images/user.png" class="nav-avatar" />
                                }
                                <b class="caret"></b></a>
                                <ul class="dropdown-menu">
                                    <li><a href="@Url.Action("ViewProfile","User")">Your Profile</a></li>
                                 
                                   <li><a href="@Url.Action("ChangePassword","User")">Change Password</a></li>
                                    <li class="divider"></li>
                                  <li><a href="@Url.Action("Logout","Account")">Logout</a></li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                    <!-- /.nav-collapse -->
                </div>
            </div>
            <!-- /navbar-inner -->
        </div>
        <!-- /navbar -->
        <div class="wrapper">
            <div class="container">
                <div class="row">
                    <div class="span3">
                        <div class="sidebar">
                            <ul class="widget widget-menu unstyled">
                                <li class="active"><a href="@Url.Action("Index","User")"><i class="menu-icon icon-dashboard"></i>User Dashboard
                                </a></li>
                                <li><a href="@Url.Action("ViewProtein")"><i class="menu-icon icon-bullhorn"></i>View Protein </a>
                                </li>
                                <li><a href="@Url.Action("ViewPayment")"><i class="menu-icon icon-inbox"></i>View Payment <b class="label green pull-right">
                                    11</b> </a></li>
                                <li><a href="@Url.Action("ViewMeasurement")"><i class="menu-icon icon-tasks"></i>View Measurement <b class="label orange pull-right">
                                    19</b> </a></li>
                            </ul>
                            <!--/.widget-nav-->
                            
                            
                            <ul class="widget widget-menu unstyled">
                              
                                <li><a href="@Url.Action("ViewAttendance")"><i class="menu-icon icon-paste"></i>View Attendance </a></li>
                                 <li><a href="@Url.Action("ViewWorkout")"><i class="menu-icon icon-paste"></i>View Workout </a></li>
                                
                                <li><a href="@Url.Action("Logout","Account")"><i class="menu-icon icon-table"></i>Logout </a></li>
                               
                            </ul>
                         
                        </div>
                        <!--/.sidebar-->
                    </div>
                    <!--/.span3-->
                    <div class="span9">


                        <div class="content">
                            @RenderBody()

                          
                            <!--/.module-->
                          
                            <!--/.module-->
                        </div>
                        <!--/.content-->
                    </div>
                    <!--/.span9-->
                </div>
            </div>
            <!--/.container-->
        </div>
        <!--/.wrapper-->
        <div class="footer">
            <div class="container">
                <b class="copyright">Gym - Management </b>All rights reserved.
            </div>
        </div>

    <script src="@Url.Content("~/scripts/jquery-ui-1.10.1.custom.min.js")"></script>
  
    <script src="@Url.Content("~/bootstrap/js/bootstrap.min.js")"></script>
  
       <script src="@Url.Content("~/scripts/jquery.unobtrusive-ajax.min.js")"></script>
     <script src="@Url.Content("~/scripts/datatables/jquery.dataTables.js")"></script>
     <script src="@Url.Content("~/scripts/common.js")"></script>
     
   
      
</body>
</html>
